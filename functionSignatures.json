    {"name":"DaysToExpiration",   "kind":"namevalue", "type":["numeric"], "purpose":"# days until contract expires (optional)"},
    {"name":"AnnualFactor",       "kind":"namevalue", "type":["numeric"], "purpose":"# of days per year (default=365)"}
  ]
},
